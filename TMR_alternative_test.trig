@prefix : <http://anonymous.org/data/> .
@prefix vocab: <http://anonymous.org/vocab/> .
@prefix vocab4i: <http://anonymous.org/vocab4i/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix data: <http://anonymous.org/data/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#    Action Types
:ActAdministerDrug  rdf:type vocab:DrugAdministrationType, owl:NamedIndividual ;
        rdfs:label "Administer drugs"@en .


#    Transition Types
:TrTransition rdf:type vocab:TransitionType, owl:NamedIndividual ;
        vocab:hasTransformableSituation :SitPrecondition ;
        vocab:hasExpectedSituation :SitPostcondition .

# causation belief
data:CBTestCausationBelief {
    data:ActAdministerDrug
        vocab:causes                  data:TrTransition .
    data:CBTestCausationBelief
        a                             vocab:CausationBelief ;
        vocab:strength                "L2"^^xsd:string;
        vocab:frequency               "always"^^xsd:string.
}

## Guideline
:CIG-Test
        rdf:type vocab:ClinicalGuideline ;
        rdfs:label "CIG for testing purposes"@en .


## Recommendation
:RecDB-Test1 {
    :RecDB-Test1
        a                             vocab:ClinicalRecommendation ;
        rdfs:label                    "test1"@en ;
        vocab:strength                "should"^^xsd:string ;
        vocab:partOf                  :CIG-Test ;
        vocab:aboutExecutionOf        :ActAdministerDrug;
        vocab:basedOn                 :CBTestCausationBelief . 
}

